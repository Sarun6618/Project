		<td>
		 <label for="City">City:</label>
		</td>
		<td>
		 <input type="text" id="City" name="City"> <br>
		</td>
	   </tr>
	 <tr>
	  <td>
	   <label for="model">Model:</label>
	  </td>
	  <td>
	   <input type="text" id="model" name="model"> <br>
	  </td>
	 </tr>
	 <tr>
	  <td>
	   <label for="TagID">Tag ID:</label>
	  </td>
	  <td>
	   <input type="text" id="TagID" name="TagID"> <br>
	  </td>
	 </tr>
	 <tr>
	  <td>
